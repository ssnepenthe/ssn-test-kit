{
    "name": "ssnepenthe/ssn-test-kit",
    "description": "Utilities for working with BrowserKit and WP-CLI within my PHPUnit tests.",
    "require": {
        "fabpot/goutte": "^3.2",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/panther": "^0.2.0",
        "symfony/process": "^4.2"
    },
    "require-dev": {
        "psy/psysh": "^0.9.9",
        "slim/slim": "^3.12"
    },
    "autoload": {
        "psr-4": { "SsnTestKit\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "SsnTestKit\\Tests\\": "tests/" }
    },
    "license": "MIT"
}
